				if (document.getElementById("icon_pacemaker").style.opacity == 0){
					close_copy();				
					document.getElementById("copy_patients").style.opacity = "1";
					document.getElementById("copy_patients").style.webkitTransform = "scale(1)";
				}
			}					
			
			
        }   
        
        
        
        function close_copy(){
            //closes copy boxes
            document.getElementById("copy_cell").style.webkitTransform = "scale(0)";
			document.getElementById("copy_cell").style.opacity = "0";
			
			document.getElementById("copy_cloud").style.webkitTransform = "scale(0)";
			document.getElementById("copy_cloud").style.opacity = "0";
			
			document.getElementById("copy_med").style.webkitTransform = "scale(0)";
			document.getElementById("copy_med").style.opacity = "0";
			
			document.getElementById("copy_mob").style.webkitTransform = "scale(0)";
			document.getElementById("copy_mob").style.opacity = "0";
			
			document.getElementById("copy_care").style.webkitTransform = "scale(0)";
			document.getElementById("copy_care").style.opacity = "0";
			
			document.getElementById("copy_patients").style.webkitTransform = "scale(0)";
			document.getElementById("copy_patients").style.opacity = "0";
        } 
        
        
        
        function stop(){
            //called when the scrollview stops scrolling and leaves this page
            window.location.reload();
        }                
        
