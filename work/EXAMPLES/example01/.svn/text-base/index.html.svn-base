        function start(){
        
        setTimeout(function(){
				document.getElementById("copy_bottom").style.opacity = "1";
			}, 13000);
        
        	
        
           var obj = document.getElementById('copy_main_left');
           obj.style.webkitTransform = "translate3d(0px, 157px, 0)";
           
           obj.addEventListener("webkitTransitionEnd", function(){
				
				//alert("hi");				
				
			},1000);
			
			
		  
           
