				document.getElementById('hot_care').className = "hot_care hotspot";
				document.getElementById('hot_patients').className = "hot_patients hotspot";
			}, 13000);
			
			document.getElementById("hot_cell").onclick = function() {
				close_copy();		
				document.getElementById("copy_cell").style.opacity = "1";
				document.getElementById("copy_cell").style.webkitTransform = "scale(1)";
			}
			document.getElementById("hot_cloud").onclick = function() {			
				close_copy();				
				document.getElementById("copy_cloud").style.opacity = "1";
				document.getElementById("copy_cloud").style.webkitTransform = "scale(1)";			
			}
