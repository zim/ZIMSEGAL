			}

			
			
			document.getElementById("icon_cell_tower").onclick = function() {
				if (document.getElementById("icon_cell_tower").style.opacity == 0){
					close_copy();		
					document.getElementById("copy_cell").style.opacity = "1";
					document.getElementById("copy_cell").style.webkitTransform = "scale(1)";
				}
			}
			document.getElementById("icon_cloud").onclick = function() {	
